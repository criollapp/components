    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.7.2",
    "rollup": "^0.55.1",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.10.0",
    "shelljs": "^0.8.1"
