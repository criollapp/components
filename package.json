  "author": "Miguel Rivera",
  "private": false,
  "dependencies": {
    "@criollapp/common": "^1.0.10",
    "zone.js": "^0.8.20",
    "@angular/cli": "^1.6.6",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/compiler-cli": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@types/jasmine": "^2.8.6",
    "clean-css": "^4.1.9",
    "html-minifier": "^3.5.8",
    "jasmine-core": "^2.9.1",
